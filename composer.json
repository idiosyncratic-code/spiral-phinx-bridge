{
  "name": "idiosyncrative/spiral-phinx-bridge",
  "type": "library",
  "license": "EUPL-1.2",
  "description": "Spiral Bridge to add Phinx DB migrations",
  "homepage": "https://github.com/idiosyncratic-code/spiral-phinx-bridge",
  "support": {
    "source": "https://github.com/idiosyncratic-code/spiral-phinx-bridge"
  },
  "require": {
    "php": ">=8.2",
    "robmorgan/phinx": "1.x-dev",
    "spiral/boot": "^3.8",
    "spiral/config": "^3.8",
    "spiral/console": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Idiosyncratic\\SpiralPhinxBridge\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Idiosyncratic\\SpiralPhinxBridge\\": "test"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "idiosyncratic/coding-standard": "^3.0",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/phpstan": "^1.10",
    "qossmic/deptrac-shim": "^1.0"
  }
}
